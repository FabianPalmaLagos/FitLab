<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>BusquedaRut</servlet-name>
        <servlet-class>Controller.BusquedaRut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroMiembro2</servlet-name>
        <servlet-class>Controller.RegistroMiembro2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroUsuario2</servlet-name>
        <servlet-class>Controller.RegistroUsuario2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteByRut</servlet-name>
        <servlet-class>Controller.DeleteByRut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroEvaluacion</servlet-name>
        <servlet-class>Controller.RegistroEvaluacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusquedaEvaluacion</servlet-name>
        <servlet-class>Controller.BusquedaEvaluacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login2</servlet-name>
        <servlet-class>Controller.Login2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userMenu</servlet-name>
        <servlet-class>Controller.userMenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userUpdate</servlet-name>
        <servlet-class>Controller.userUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BusquedaRut</servlet-name>
        <url-pattern>/BusquedaRut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroMiembro2</servlet-name>
        <url-pattern>/RegistroMiembro2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroUsuario2</servlet-name>
        <url-pattern>/RegistroUsuario2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteByRut</servlet-name>
        <url-pattern>/DeleteByRut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroEvaluacion</servlet-name>
        <url-pattern>/RegistroEvaluacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaEvaluacion</servlet-name>
        <url-pattern>/BusquedaEvaluacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login2</servlet-name>
        <url-pattern>/Login2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userMenu</servlet-name>
        <url-pattern>/userMenu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userUpdate</servlet-name>
        <url-pattern>/userUpdate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
